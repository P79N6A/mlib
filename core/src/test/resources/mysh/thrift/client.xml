<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="_client2Fact" class="mysh.thrift.ThriftClientFactory">
		<constructor-arg name="conf">
			<bean class="mysh.thrift.ThriftClientFactory$Config"
			      p:serverHost="localhost" p:serverPort="19090"
			      p:clientSocketTimeout="5000"
			      p:iface="mysh.thrift.TService1$Iface"
			      p:TClientClass="mysh.thrift.TService1$Client"
			      p:useTLS="true"
			      p:trustKeyStore="classpath:mysh/thrift/rootPublic.jks" p:trustKeyStorePw="123456"
			      p:requireClientAuth="true"
			      p:selfKeyStore="classpath:mysh/thrift/client.jks" p:selfKeyStorePw="client"
							/>
		</constructor-arg>
	</bean>
	<bean id="client2" factory-bean="_client2Fact" factory-method="buildPooled"/>

</beans>
